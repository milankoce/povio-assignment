{
  "devDependencies": {
    "cypress": "^13.17.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-real-events": "^1.14.0",
    "mocha": "^11.0.1",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "cypress": "cypress open",
    "all:e2e": "cypress run --headed --browser chrome --spec cypress/e2e/*.cy.ts",
    "all:e2e:headless": "cypress run --headless --browser chrome --spec cypress/e2e/*.cy.ts",
    "format": "prettier --write 'cypress/**/*.ts'"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "chai": "^5.1.2"
  }
}
